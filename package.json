{
  "name": "kythia",
  "version": "0.9.12-beta",
  "description": "Kythia Hye-Jin! Your sweet, cute, and beautiful companion! ",
  "main": "index.js",
  "author": {
    "name": "kenndeclouv",
    "email": "kenndeclouv@gmail.com"
  },
  "license": "EULA",
  "bin": {
    "kythia": "./kythia-cli.js"
  },
  "scripts": {
    "start": "node index.js --deploy",
    "start:bun": "bun index.js --deploy",
    "kythia": "node kythia-cli.js",
    "format": "biome format --write **/*.js",
    "fix": "biome check --write **/*.js --unsafe"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "@google/genai": "^1.20.0",
    "@sentry/node": "^10.10.0",
    "async-mutex": "^0.5.0",
    "axios": "^1.12.2",
    "canvas": "^3.2.0",
    "cli-color": "^2.0.4",
    "connect-session-sequelize": "^7.1.7",
    "discord.js": "^14.25.0",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "figlet": "^1.9.3",
    "javascript-obfuscator": "^4.1.1",
    "kythia-core": "^0.9.6-beta",
    "luxon": "^3.7.2",
    "marked": "^4.3.0",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "poru": "^5.2.0",
    "poru-spotify": "^2.0.2",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "tmp": "^0.2.5",
    "topgg-autoposter": "^2.0.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "youtube-transcript": "^1.2.1"
  },
  "devDependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@biomejs/biome": "^2.3.6",
    "glob": "^11.0.3"
  },
  "_moduleAliases": {
    "@src": "src",
    "@addons": "addons",
    "@utils": "src/utils",
    "@coreHelpers": "addons/core/helpers",
    "@coreModels": "addons/core/database/models",
    "@coreDatabase": "addons/core/database",
    "@srcDatabase": "src/database"
  },
  "jest": {
    "moduleNameMapper": {
      "^@src/(.*)$": "<rootDir>/src/$1",
      "^@addons/(.*)$": "<rootDir>/addons/$1",
      "^@utils/(.*)$": "<rootDir>/src/utils/$1",
      "^@coreHelpers/(.*)$": "<rootDir>/addons/core/helpers/$1",
      "^@coreModels/(.*)$": "<rootDir>/addons/core/database/models/$1",
      "^@coreDatabase/(.*)$": "<rootDir>/addons/core/database/$1",
      "^@srcDatabase/(.*)$": "<rootDir>/src/database/$1"
    }
  },
  "keywords": [
    "discord",
    "bot",
    "kythia",
    "music",
    "music bot",
    "discord bot",
    "discord.js",
    "discord music bot",
    "discord.js music bot",
    "lavalink",
    "lavalink bot",
    "lavalink discord bot",
    "lavalink music bot",
    "lavalink discord.js",
    "lavalink discord.js music bot",
    "poru",
    "poru bot",
    "poru discord bot",
    "poru discord.js",
    "poru discord.js music bot"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kythia/kythia.git"
  },
  "bugs": {
    "url": "https://github.com/kythia/kythia#issues"
  },
  "homepage": "https://github.com/kythia/kythia/#readme",
  "trustedDependencies": [
    "@sentry-internal/node-cpu-profiler",
    "es5-ext",
    "javascript-obfuscator",
    "unrs-resolver"
  ]
}
